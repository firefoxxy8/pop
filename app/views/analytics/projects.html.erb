<h3>People Assigned per Project</h3>

<div class="row lead">
	<div class="col-md-3">
		<%= render(:partial => 'work_items/week_navigator',
			:locals => {
				pathPrev: analytics_projects_path(:week => @weekPrev, :year => @yearPrev ),
				pathNext: analytics_projects_path(:week => @weekNext, :year => @yearNext )
			}) %>
	</div>
	<div class="col-md-2">
